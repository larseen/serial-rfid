{
  "name": "node-babel-starter",
  "version": "1.0.2",
  "description": "Node.js + Babel 6 (es2015, stage-0, react) + Koa",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "pm2 start dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n7olkachev/node-babel-starter.git"
  },
  "keywords": [
    "node",
    "babel"
  ],
  "author": "Nick Tolkachev <n7olkachev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n7olkachev/node-babel-starter/issues"
  },
  "homepage": "https://github.com/n7olkachev/node-babel-starter#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "koa": "^2.0.0",
    "lodash": "^4.15.0",
    "serialport": "^4.0.1"
  }
}
